@prefix : <http://example.org/def/base#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/def/base#> .

<http://example.org/def/base#> rdf:type owl:Ontology ;
                                <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0> ;
                                mod:createdWith <https://chowlk.linkeddata.es/> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms
<http://purl.org/dc/terms> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
dc:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  https://w3id.org/mod#createdWith
mod:createdWith rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://data.europa.eu/snb/model/elm/hasPart
<http://data.europa.eu/snb/model/elm/hasPart> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf :isPartOf ;
                                              rdfs:domain owl:Thing ;
                                              rdfs:range owl:Thing ;
                                              rdfs:comment "An association property, that defines a part/whole relationship between instances of the same class. A related resource that is included either physically or logically in the described resource." ;
                                              rdfs:label "has part" .


###  http://example.org/def/base#covers
:covers rdf:type owl:ObjectProperty ;
        owl:inverseOf :isCoveredBy ;
        rdfs:domain :Subject ;
        rdfs:range <https://example.org/def/base#LearningAchievement> ;
        rdfs:comment "An object property that" ;
        rdfs:label "covers" .


###  http://example.org/def/base#entryRequirement
:entryRequirement rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isRequiredBy ;
                  rdfs:domain :Program ;
                  rdfs:range :LearningEntitlement ;
                  rdfs:comment "Specific entry requirement or prerequisite of individuals which this learning entitlement is dessigned to start this learning" ;
                  rdfs:label "entry requirement" .


###  http://example.org/def/base#evaluates
:evaluates rdf:type owl:ObjectProperty ;
           owl:inverseOf :isEvaluatedBy ;
           rdfs:domain :LearningAssessment ;
           rdfs:range :Subject ;
           rdfs:comment "The inverse property of is evaluated by" ;
           rdfs:label "evaluates" .


###  http://example.org/def/base#hasAgreementWith
:hasAgreementWith rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ;
                  rdfs:domain :Organization ;
                  rdfs:range :Organization ;
                  rdfs:comment "All agreements in this ontology are modeled as mutual between the involved parties." ,
                               "An object property describring the relationship between educational institutions or organizations for joint programs, credit transfer (articulation), research, student/faculty exchange, or shared curriculum, ensuring clear understanding for collaboration, like guaranteeing transfer credits for students" ;
                  rdfs:label "has agreement with" .


###  http://example.org/def/base#hasBiggerPart
:hasBiggerPart rdf:type owl:ObjectProperty ;
               owl:inverseOf :isSubpartOf ;
               rdfs:domain owl:Thing ;
               rdfs:range owl:Thing ;
               rdfs:comment "The inversy property of is subpart of" ;
               rdfs:label "has bigger part" .


###  http://example.org/def/base#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Organization ;
           rdfs:range <https://example.org/def/base#Person> ;
           rdfs:comment "the inverse property of is memeber of" ;
           rdfs:label "has member" .


###  http://example.org/def/base#hasMode
:hasMode rdf:type owl:ObjectProperty ;
         owl:inverseOf :isModalityOf ;
         rdfs:domain :Subject ;
         rdfs:range :DeliveryMode ;
         rdfs:comment "An object property defining how something is done" ;
         rdfs:label "has mode" .


###  http://example.org/def/base#hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :participatesIn ;
                rdfs:domain :Program ;
                rdfs:range :External ;
                rdfs:comment "The inverse property of participates in" ;
                rdfs:label "has participant" .


###  http://example.org/def/base#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         owl:inverseOf :isRoleOf ;
         rdfs:domain <https://example.org/def/base#Person> ;
         rdfs:range :Role ;
         rdfs:comment "An object property that indicates someone has a function" ;
         rdfs:label "has role" .


###  http://example.org/def/base#hasStudent
:hasStudent rdf:type owl:ObjectProperty ;
            owl:inverseOf :isEnrolledIn ;
            rdfs:domain :KnowledgeGrouping ;
            rdfs:range :Student ;
            rdfs:comment "The inverse property of is enrolled in" ;
            rdfs:label "has student" .


###  http://example.org/def/base#isCoveredBy
:isCoveredBy rdf:type owl:ObjectProperty ;
             rdfs:domain <https://example.org/def/base#LearningAchievement> ;
             rdfs:range :Subject ;
             rdfs:comment "The inverse property of covers" ;
             rdfs:label "is covered by" .


###  http://example.org/def/base#isEnrolledIn
:isEnrolledIn rdf:type owl:ObjectProperty ;
              rdfs:domain :Student ;
              rdfs:range :KnowledgeGrouping ;
              rdfs:comment "An oBject property that indicates a student is enrolled in a Knowledge Grouping" ;
              rdfs:label "is enrolled in" .


###  http://example.org/def/base#isEvaluatedBy
:isEvaluatedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Subject ;
               rdfs:range :LearningAssessment ;
               rdfs:comment "An object property that indicates how a subject is evaluated" ;
               rdfs:label "is evaluated by" .


###  http://example.org/def/base#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain <https://example.org/def/base#Person> ;
            rdfs:range :Organization ;
            rdfs:comment "An object property that says someone belongs to a project, group or Organization" ;
            rdfs:label "is member of" .


###  http://example.org/def/base#isModalityOf
:isModalityOf rdf:type owl:ObjectProperty ;
              rdfs:domain :DeliveryMode ;
              rdfs:range :Subject ;
              rdfs:comment "The inverse property of has mode" ;
              rdfs:label "is modality of" .


###  http://example.org/def/base#isOfferedBy
:isOfferedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :offers ;
             rdfs:domain :Program ;
             rdfs:range :Internal ;
             rdfs:comment "The inverse property of offers" ;
             rdfs:label "is offered by" .


###  http://example.org/def/base#isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:domain owl:Thing ;
          rdfs:range owl:Thing ;
          rdfs:comment "Inverse of has part" ;
          rdfs:label "is part of" .


###  http://example.org/def/base#isRequiredBy
:isRequiredBy rdf:type owl:ObjectProperty ;
              rdfs:domain :LearningEntitlement ;
              rdfs:range :Program ;
              rdfs:comment "The inverse property of entry requirement" ;
              rdfs:label "is required by" .


###  http://example.org/def/base#isResponabilityOf
:isResponabilityOf rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isResponsibleFor ;
                   rdfs:domain :Coordinator ;
                   rdfs:range :KnowledgeGrouping ;
                   rdfs:comment "An object property that sais someone o something is responsibility of someone or something" ;
                   rdfs:label "is responability of" .


###  http://example.org/def/base#isResponsibleFor
:isResponsibleFor rdf:type owl:ObjectProperty ;
                  rdfs:domain :KnowledgeGrouping ;
                  rdfs:range :Coordinator ;
                  rdfs:comment "An object property that sais someone o something is responsible for someone or something" ;
                  rdfs:label "is responsible for" .


###  http://example.org/def/base#isRoleOf
:isRoleOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Role ;
          rdfs:range <https://example.org/def/base#Person> ;
          rdfs:comment "The inverse property of has role" ;
          rdfs:label "is role of" .


###  http://example.org/def/base#isSubpartOf
:isSubpartOf rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain owl:Thing ;
             rdfs:range owl:Thing ;
             rdfs:comment "A \"part of\" property but between academic entities" ;
             rdfs:label "is subpart of" .


###  http://example.org/def/base#isTaughtBy
:isTaughtBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :teaches ;
            rdfs:domain :Subject ;
            rdfs:range :Professor ;
            rdfs:comment "The inverse property of teaches" ;
            rdfs:label "is taught by" .


###  http://example.org/def/base#isWorkplaceOf
:isWorkplaceOf rdf:type owl:ObjectProperty ;
               owl:inverseOf :roleAt ;
               rdfs:domain owl:Thing ;
               rdfs:range owl:Thing ;
               rdfs:comment "The inverse property of role at" ;
               rdfs:label "is workplace of" .


###  http://example.org/def/base#offers
:offers rdf:type owl:ObjectProperty ;
        rdfs:domain :Internal ;
        rdfs:range :Program ;
        rdfs:comment "An object property describes the relation between an Internal Organization and a Program (Master´s Degree)" ;
        rdfs:label "offers" .


###  http://example.org/def/base#participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:domain :External ;
                rdfs:range :Program ;
                rdfs:comment "An object property that indicates someone is participating in a project, program, group..." ;
                rdfs:label "participates in" .


###  http://example.org/def/base#roleAt
:roleAt rdf:type owl:ObjectProperty ;
        rdfs:domain owl:Thing ;
        rdfs:range owl:Thing ;
        rdfs:comment "An object property that indicates someone has a function in a group, project, entity or Organization" ;
        rdfs:label "role at" .


###  http://example.org/def/base#teaches
:teaches rdf:type owl:ObjectProperty ;
         rdfs:domain :Professor ;
         rdfs:range :Subject ;
         rdfs:comment "An object property that indicates that someone teaches a subject" ;
         rdfs:label "teaches" .


###  http://purl.org/cerif/frapo/employs
<http://purl.org/cerif/frapo/employs> rdf:type owl:ObjectProperty ;
                                      owl:inverseOf <http://purl.org/cerif/frapo/isEmployedBy> ;
                                      rdfs:domain :Organization ;
                                      rdfs:range <https://example.org/def/base#Person> ;
                                      rdfs:comment "An object property relating an employing agent to an employed agent, for example a person or an organization, who is typically employed to undertaking specified work in exchange for payment." ;
                                      rdfs:label "employs" .


###  http://purl.org/cerif/frapo/isEmployedBy
<http://purl.org/cerif/frapo/isEmployedBy> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <https://example.org/def/base#Person> ;
                                           rdfs:range :Organization ;
                                           rdfs:comment "An object property relating an employed agent, typically a person or an organization, to an employing agent." ;
                                           rdfs:label "is employed by" .


#################################################################
#    Data properties
#################################################################

###  http://data.europa.eu/snb/model/elm/workload
<http://data.europa.eu/snb/model/elm/workload> rdf:type owl:DatatypeProperty ;
                                               rdfs:domain <http://example.org/def/http./example.org/def/base#LearningActivity> ;
                                               rdfs:range xsd:integer ;
                                               rdfs:comment "The actual workload in number of hours the learner has spent engaged in the activity. This would include the number of hours in class, in group work, in practicals, as well as hours engaged in self-motivated study." ;
                                               rdfs:label "workload" .


###  http://example.org/def/base#adress
:adress rdf:type owl:DatatypeProperty ;
        rdfs:domain :AcademicEntity ;
        rdfs:range xsd:string ;
        rdfs:comment "A date property defining the adress of a place" ;
        rdfs:label "adress" .


###  http://example.org/def/base#credits
:credits rdf:type owl:DatatypeProperty ;
         rdfs:domain :KnowledgeGrouping ;
         rdfs:range xsd:integer ;
         rdfs:comment "A date property defining the number of credits of a Knowledge Grouping" ;
         rdfs:label "credits" .


###  http://example.org/def/base#endDate
:endDate rdf:type owl:DatatypeProperty ;
         rdfs:domain :Role ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "A data property defining a end date" ;
         rdfs:label "end date" .


###  http://example.org/def/base#maxScore
:maxScore rdf:type owl:DatatypeProperty ;
          rdfs:domain :LearningAssessment ;
          rdfs:range xsd:integer ;
          rdfs:comment "A data property defining the weight of a Learning Assesment" ;
          rdfs:label "max score" .


###  http://example.org/def/base#startDate
:startDate rdf:type owl:DatatypeProperty ;
           rdfs:domain :Role ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "A data property defining a start date" ;
           rdfs:label "start date" .


###  http://example.org/def/base#submissionDate
:submissionDate rdf:type owl:DatatypeProperty ;
                rdfs:domain :LearningAssessment ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "A data property defining a submission date of a Learning Assesment" ;
                rdfs:label "submission date" .


###  http://purl.org/cerif/frapo/hasCode
<http://purl.org/cerif/frapo/hasCode> rdf:type owl:DatatypeProperty ;
                                      rdfs:domain :Subject ;
                                      rdfs:range xsd:string ;
                                      rdfs:comment "A data property defining a code" ;
                                      rdfs:label "has code" .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:DatatypeProperty ;
                                 rdfs:domain owl:Thing ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "A word or set of words by which a person or thing is known, addressed, or referred to." ;
                                 rdfs:label "name" .


#################################################################
#    Classes
#################################################################

###  http://data.europa.eu/snb/model/elm/LearningAchievement
<http://data.europa.eu/snb/model/elm/LearningAchievement> rdf:type owl:Class ;
                                                          owl:equivalentClass <https://example.org/def/base#LearningAchievement> ;
                                                          rdfs:isDefinedBy <http://data.europa.eu/snb/model/elm/> ;
                                                          rdfs:label "Learning Achievement" .


###  http://data.europa.eu/snb/model/elm/LearningActivity
<http://data.europa.eu/snb/model/elm/LearningActivity> rdf:type owl:Class ;
                                                       owl:equivalentClass <http://example.org/def/http./example.org/def/base#LearningActivity> ;
                                                       rdfs:isDefinedBy <http://data.europa.eu/snb/model/elm/> ;
                                                       rdfs:label "Learning Activity" .


###  http://data.europa.eu/snb/model/elm/LearningAssessment
<http://data.europa.eu/snb/model/elm/LearningAssessment> rdf:type owl:Class ;
                                                         owl:equivalentClass :LearningAssessment ;
                                                         rdfs:isDefinedBy <http://data.europa.eu/snb/model/elm/> ;
                                                         rdfs:label "Learning Assessment" .


###  http://data.europa.eu/snb/model/elm/LearningEntitlement
<http://data.europa.eu/snb/model/elm/LearningEntitlement> rdf:type owl:Class ;
                                                          owl:equivalentClass :LearningEntitlement ;
                                                          rdfs:isDefinedBy <http://data.europa.eu/snb/model/elm/> ;
                                                          rdfs:label "Learning Entitlement" .


###  http://example.org/def/base#AcademicEntity
:AcademicEntity rdf:type owl:Class ;
                rdfs:comment "A group of people that shares functions related to the Academic world" ;
                rdfs:label "Academic Entity" .


###  http://example.org/def/base#AcademicRole
:AcademicRole rdf:type owl:Class ;
              rdfs:subClassOf :Role ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :roleAt ;
                                owl:someValuesFrom :AcademicEntity
                              ] ;
              rdfs:comment "A subclass of role that is related to the Master´s Degree" ;
              rdfs:label "Academic Role" .


###  http://example.org/def/base#Assesment
:Assesment rdf:type owl:Class ;
           rdfs:subClassOf :LearningAssessment ;
           rdfs:comment "A way to evaluate a student about the contents of a subject" ;
           rdfs:label "Assesment" .


###  http://example.org/def/base#Compulsory
:Compulsory rdf:type owl:Class ;
            rdfs:subClassOf :DeliveryMode ;
            owl:disjointWith :Optional ;
            rdfs:comment "A Delivery Mode that says that a student needs to be enrolled in the subject. " ;
            rdfs:label "Compulsory" .


###  http://example.org/def/base#Coordinator
:Coordinator rdf:type owl:Class ;
             rdfs:subClassOf :AcademicRole ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :isResponsibleFor ;
                               owl:someValuesFrom :AcademicEntity
                             ] ;
             owl:disjointWith :Student ;
             rdfs:comment "An Academic role that is responsible of a KnowledgeGrouping" ;
             rdfs:label "Coordinator" .


###  http://example.org/def/base#Course
:Course rdf:type owl:Class ;
        rdfs:subClassOf :KnowledgeGrouping ;
        rdfs:comment "A knowledge group that includes what is learn in a course (2024-2025)" ;
        rdfs:label "Course" .


###  http://example.org/def/base#DeliveryMode
:DeliveryMode rdf:type owl:Class ;
              rdfs:comment "The mode in what a subject is delivered" ;
              rdfs:label "Delivery Mode" .


###  http://example.org/def/base#Department
:Department rdf:type owl:Class ;
            owl:equivalentClass <http://purl.org/cerif/frapo/Department> ;
            rdfs:subClassOf :AcademicEntity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :isSubpartOf ;
                              owl:someValuesFrom <http://purl.org/cerif/frapo/Faculty>
                            ] ;
            rdfs:comment "A department or sub-section within a larger organization, for example a university academic department." ;
            rdfs:label "Department" .


###  http://example.org/def/base#Enterprise
:Enterprise rdf:type owl:Class ;
            rdfs:subClassOf :External ;
            rdfs:comment "A subclass External Organization that is private" ;
            rdfs:label "Enterprise" .


###  http://example.org/def/base#Exam
:Exam rdf:type owl:Class ;
      rdfs:subClassOf :LearningAssessment ;
      rdfs:comment "A type of assesment" ;
      rdfs:label "Exam" .


###  http://example.org/def/base#External
:External rdf:type owl:Class ;
          rdfs:subClassOf :Organization ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :participatesIn ;
                            owl:someValuesFrom :Program
                          ] ;
          rdfs:comment "A Organization that is not directly responsible of a Master´s Degree but collaborates." ;
          rdfs:label "External" .


###  http://example.org/def/base#Faculty
:Faculty rdf:type owl:Class ;
         owl:equivalentClass <http://purl.org/cerif/frapo/Faculty> ;
         rdfs:subClassOf :AcademicEntity ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :isSubpartOf ;
                           owl:someValuesFrom <http://purl.org/cerif/frapo/University>
                         ] ;
         rdfs:comment "A grouping of similar departments within a university context, also known as a school or a division, for example the Humanities Division, the Mathematics Faculty, or the School of Medicine." ;
         rdfs:label "Faculty" .


###  http://example.org/def/base#Internal
:Internal rdf:type owl:Class ;
          rdfs:subClassOf :Organization ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :offers ;
                            owl:someValuesFrom :Program
                          ] ;
          rdfs:comment "A Organization that is responsible of a Master´s Degree and offers it to students." ;
          rdfs:label "Internal" .


###  http://example.org/def/base#KnowledgeGrouping
:KnowledgeGrouping rdf:type owl:Class ;
                   rdfs:comment "A KnowledgeGrouping may be contained by another KnowledgeGrouping or an organizationalUnit using knowledgeGrouping." ;
                   rdfs:label "Knowledge Grouping" .


###  http://example.org/def/base#Laboratory
:Laboratory rdf:type owl:Class ;
            rdfs:subClassOf <http://example.org/def/http./example.org/def/base#LearningActivity> ;
            rdfs:comment "A subclass of Learning Activity" ;
            rdfs:label "Laboratory" .


###  http://example.org/def/base#LearningAssessment
:LearningAssessment rdf:type owl:Class ;
                    rdfs:comment "The process of establishing the extent to which a learner has attained particular knowledge, skills and competences against criteria such as learning outcomes or standards of competence." ;
                    rdfs:label "Learning Assessment" .


###  http://example.org/def/base#LearningEntitlement
:LearningEntitlement rdf:type owl:Class ;
                     rdfs:comment "A right, e.g., to practice a profession, take advantage of a learning opportunity or join an organisation, as a result of the acquisition of knowledge, skills, responsibility and/or autonomy." ;
                     rdfs:label "Learning Entitlement" .


###  http://example.org/def/base#Lecture
:Lecture rdf:type owl:Class ;
         rdfs:subClassOf <http://example.org/def/http./example.org/def/base#LearningActivity> ;
         rdfs:comment "A type of Learning Activity" ;
         rdfs:label "Lecture" .


###  http://example.org/def/base#OnCampus
:OnCampus rdf:type owl:Class ;
          rdfs:subClassOf :DeliveryMode ;
          rdfs:comment "A presencial delivery mode of a subject" ;
          rdfs:label "On- Campus" .


###  http://example.org/def/base#Online
:Online rdf:type owl:Class ;
        rdfs:subClassOf :DeliveryMode ;
        rdfs:comment "A online delivery mode of a subject" ;
        rdfs:label "Online" .


###  http://example.org/def/base#Optional
:Optional rdf:type owl:Class ;
          rdfs:subClassOf :DeliveryMode ;
          rdfs:comment "A kind of Delivery Mode that means that a subject is not compulsoty in the program" ;
          rdfs:label "Optional" .


###  http://example.org/def/base#Organization
:Organization rdf:type owl:Class ;
              owl:equivalentClass <http://xmlns.com/foaf/0.1/Organization> ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :hasMember ;
                                owl:someValuesFrom <https://example.org/def/base#Person>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://xmlns.com/foaf/0.1/name> ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ;
              rdfs:comment "Definition of Organization" ;
              rdfs:label "Organization" .


###  http://example.org/def/base#ProfessionalRole
:ProfessionalRole rdf:type owl:Class ;
                  rdfs:subClassOf :Role ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :roleAt ;
                                    owl:someValuesFrom :Organization
                                  ] ;
                  rdfs:comment "A role not related to the Master´s Degree" ;
                  rdfs:label "Professional Role" .


###  http://example.org/def/base#Professor
:Professor rdf:type owl:Class ;
           rdfs:subClassOf :AcademicRole ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :teaches ;
                             owl:someValuesFrom :Subject
                           ] ;
           owl:disjointWith :Student ;
           rdfs:comment "An Academic role that teaches a subject" ;
           rdfs:label "Professor" .


###  http://example.org/def/base#Program
:Program rdf:type owl:Class ;
         rdfs:subClassOf :KnowledgeGrouping ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :entryRequirement ;
                           owl:someValuesFrom :LearningEntitlement
                         ] ;
         rdfs:comment "A structured, coordinated set of courses and activities leading to specific learning outcomes, often culminating in a degree" ;
         rdfs:label "Program" .


###  http://example.org/def/base#ResearchGroup
:ResearchGroup rdf:type owl:Class ;
               owl:equivalentClass <http://purl.org/cerif/frapo/ResearchGroup> ;
               rdfs:subClassOf :AcademicEntity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isSubpartOf ;
                                 owl:someValuesFrom :Department
                               ] ;
               rdfs:comment "A group of people, usually within a university department or a research institute, and typically under a single group leader, dedicated to working together to undertake research on one or more topics. May also be called a research unit." ;
               rdfs:label "Research Group" .


###  http://example.org/def/base#Role
:Role rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :startDate ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:dateTime
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :endDate ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:dateTime
                      ] ;
      rdfs:comment "The function a person does in a group, project or organization" ;
      rdfs:label "Role" .


