    <!-- http://data.europa.eu/snb/model/elm/hasPart -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/snb/model/elm/hasPart">
        <owl:inverseOf rdf:resource="http://example.org/def/base#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>An association property, that defines a part/whole relationship between instances of the same class. A related resource that is included either physically or logically in the described resource.</rdfs:comment>
        <rdfs:label>has part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/def/base#covers -->

    <owl:ObjectProperty rdf:about="http://example.org/def/base#covers">
        <owl:inverseOf rdf:resource="http://example.org/def/base#isCoveredBy"/>
        <rdfs:domain rdf:resource="http://example.org/def/base#Subject"/>
        <rdfs:range rdf:resource="https://example.org/def/base#LearningAchievement"/>
        <rdfs:comment>An object property that</rdfs:comment>
        <rdfs:label>covers</rdfs:label>
    </owl:ObjectProperty>
    


